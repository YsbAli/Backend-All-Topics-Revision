<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users Index Page</title>
</head>
<body>

    <!-- <% %>    --- for writing js -->
    <!-- <%= %>   --- for printing js  -->
<h2>Users List</h2>
  <div class="space">
    <h3><a href="/users/create">Create New Task</a></h3>
  </div>

    <table>
       <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
       </thead>
       <tbody>
        <% for (user of users){ %>

            <tr>
                <td><%= user.first_name %></td>
                <td><%= user.last_name %></td>
                <td><%= user.email %></td>
                <td><%= user.age %></td>
                <!-- <td><%= user.fistname %></td> -->
                <td> <a href="edit.html?id=1">Update <%= user.first_name %> <%= user.last_name %>.</a></td>
                <td> <a href="delete.html?id=1">Delete <%= user.first_name %> <%= user.last_name %>.</a></td>

            </tr>
        <% } %>
       </tbody>
    </table>


    
</body>
</html>